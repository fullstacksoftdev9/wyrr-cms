@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style type="text/css">
    .popup-inner {
        width: 900px;
        max-width: 55%;
        padding: 2px;
        position: absolute;
        top: 50%;
        left: 50%;
        -webkit-transform: translate(-50%, -50%);
        transform: translate(-50%, -50%);
        box-shadow: 0px 2px 6px rgba(0,0,0,1);
        border-radius: 3px;
        background: #fff;
        max-height: 50%;
        min-height: 170px;
    }
</style>

@section Styles {
    <link href="../Content/datatables/dataTables.bootstrap.css" rel="stylesheet" type="text/css" />
    <link href="../Content/toastr.min.css" rel="stylesheet" type="text/css" />


}

<script src="../Scripts/jquery-2.2.3.js"></script>
<script type="text/javascript" src="~/js/StellarMaster.js"></script>


<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Stellar Master Users
    </h1>
    <ol class="breadcrumb">
        <li><a href="@Url.Action("Index", "Admin")"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="#">Stellar Master Users</a></li>
    </ol>
</section>


<!-- Main content -->
<section class="content" style="height:100% !important">
    <div class="row">
        <div class="col-xs-12">
            <div class="box box-primary">
                <div class="box-header text-right">
                    <div class="pull-right text-right pad">



                    </div>
                    <div class="box-title pull-left">
                        @*<span id="pageTitle">@UserSession.Current.CompanyName</span>*@
                    </div>
                </div>
                <div class="box-body">
                    <div class="row">
                        <div class="col-sm-12 table-responsive">
                            <table cellpadding="0" cellspacing="0" border="0" id="grid" class="table table-bordered table-hover" style="width:100%;">
                                <thead>
                                    <tr>
                                        <th width="10%">
                                            Public Key
                                        </th>
                                        <th width="10%">
                                            Secret Key
                                        </th>
                                        <th width="15%">
                                            Action
                                        </th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>

                </div><!-- /.box-body -->
            </div><!-- /.box -->
        </div>
    </div>
</section><!-- /.content -->


<div class="popup" data-popup="popup">
    <div class="popup-inner">
        <div class="row">
            <div id="partialContent" class="col-md-12">

            </div><!--/.col (right) -->
        </div>
        <a class="popup-close" href="#" onclick="ClosePopup();">x</a>
    </div>
</div>



@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="../Scripts/jquery-2.2.3.js"></script>

@Scripts.Render("~/bundles/usertransactionsetting")
<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        User Transaction Setting
    </h1>
    <ol class="breadcrumb">
        <li><a href="@Url.Action("Index", "UserTransactionSetting")"><i class="fa fa-dashboard"></i>Home</a></li>
        <li><a href="#">User Transaction setting</a></li>
    </ol>
</section>


<!-- Main content -->
<section class="content" style="height:100% !important">
    <div class="row">
        <div class="col-xs-12">
            <div class="box box-primary">
                <div class="box-header text-right">
                    <div class="pull-right text-right pad">



                    </div>
                    <div class="box-title pull-left">
                        @*<span id="pageTitle">@UserSession.Current.CompanyName</span>*@
                    </div>
                </div>
                <div class="box-body">
                    <div class="row">
                        <div class="col-sm-12 table-responsive">
                            <table cellpadding="0" cellspacing="0" border="0" id="grid" class="table table-bordered table-hover" style="width:100%;">
                                <thead>
                                    <tr>
                                        <th width="10%">
                                                Username
                                            </th>
                                            <th width="10%">
                                                Transaction Per Day
                                            </th>
                                           <th width="10%">
                                            Transaction Amount Per Day ($)
                                          </th>
                                           <th width="15%">
                                            Action
                                         </th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>

                </div><!-- /.box-body -->
            </div><!-- /.box -->
        </div>
    </div>
</section><!-- /.content -->


<div class="popup" data-popup="popup">
    <div class="popup-inner">
        <div class="row">
            <div id="partialContent" class="col-md-12">

            </div><!--/.col (right) -->
        </div>
        <a class="popup-close" href="#" onclick="ClosePopup();">x</a>
    </div>
</div>





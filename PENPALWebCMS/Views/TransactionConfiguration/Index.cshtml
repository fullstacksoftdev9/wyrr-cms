
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style type="text/css">
    .popup-inner {
    width:900px;
    max-width:90%;
    padding:2px;
    position:absolute;
    top:50%;
    left:50%;
    -webkit-transform:translate(-50%, -50%);
    transform:translate(-50%, -50%);
    box-shadow:0px 2px 6px rgba(0,0,0,1);
    border-radius:3px;
    background:#fff;
    max-height:100%;
    min-height:170px;

}

</style>

<script src="../Scripts/jquery-2.2.3.js"></script>
@Scripts.Render("~/bundles/transactionconfiguration")
<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Transaction Setting
    </h1>
    <ol class="breadcrumb">
        <li><a href="@Url.Action("Index", "TransactionConfiguration")"><i class="fa fa-dashboard"></i>Home</a></li>
        <li><a href="#">Transaction Setting</a></li>
    </ol>
</section>


<!-- Main content -->
<section class="content" style="height:100% !important">
    <div class="row">
        <div class="col-xs-12">
            <div class="box box-primary">
                <div class="box-header text-right">
                    <div class="pull-right text-right pad">



                    </div>
                    <div class="box-title pull-left">
                        @*<span id="pageTitle">@UserSession.Current.CompanyName</span>*@
                    </div>
                </div>
                <div class="box-body">
                    <div class="row">
                        <div class="col-sm-12 table-responsive">
                            <table cellpadding="0" cellspacing="0" border="0" id="grid" class="table table-bordered table-hover" style="width:100%;">
                                <thead>
                                    <tr>
                                        <th width="13%">
                                            Service Tax Per Transaction 
                                        </th>
                                        <th width="15%">
                                           Minimum Amount For Transaction($)
                                        </th>
                                        <th width="10%">
                                            PayPal Margin(%)
                                        </th>
                                        <th width="10%">
                                            Minimum Amount For PayPal Transaction($)
                                        </th>
                                        <th width="10%">
                                            Coinbase Margin(%)
                                        </th>
                                        <th width="10%">
                                            Enable Add PayPal
                                        </th>
                                        <th width="10%">
                                            Enable Withdraw PayPal
                                        </th>
                                        <th width="10%">
                                            Enable Add Coinbase
                                        </th>
                                        <th width="10%">
                                            Coinbase Send Limit
                                        </th>
                                        <th width="12%">
                                            Action
                                        </th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>

                </div><!-- /.box-body -->
            </div><!-- /.box -->
        </div>
    </div>
</section><!-- /.content -->


<div class="popup" data-popup="popup">
    <div class="popup-inner">
        <div class="row">
            <div id="partialContent" class="col-md-12">

            </div><!--/.col (right) -->
        </div>
        <a class="popup-close" href="#" onclick="ClosePopup();">x</a>
    </div>
</div>


